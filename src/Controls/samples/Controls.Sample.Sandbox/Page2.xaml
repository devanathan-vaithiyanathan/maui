<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Maui.Controls.Sample.Page2"
             xmlns:vm="clr-namespace:Maui.Controls.Sample.ViewModels"
             x:DataType="vm:Page2ViewModel"
             Title="Page 2">

    <ContentPage.Content>
        <VerticalStackLayout>
            <Label Text="Page2"/>
            <Button Text="Navigate to next page" Command="{Binding NavigateForwardAsyncCommand}"/>
            <Button Text="Push Modal" Clicked="Button_Clicked"/>
        </VerticalStackLayout>
    </ContentPage.Content>

</ContentPage>
