<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        x:Name="flyoutPage"
        x:Class="Maui.Controls.Sample.Issues.Issue28440">

        <FlyoutPage.Flyout>
            <ContentPage Title="Menu">
                <StackLayout>
                    <Label Text="Flyout Menu" AutomationId="flyoutMenu" />
                </StackLayout>
            </ContentPage>
        </FlyoutPage.Flyout>

        <FlyoutPage.Detail>
            <NavigationPage>
                <x:Arguments>
                    <ContentPage>
                        <StackLayout>
                            <Button Text="Open Menu" AutomationId="Button" Clicked="Button_Clicked"/>
                        </StackLayout>
                    </ContentPage>
                </x:Arguments>
            </NavigationPage>
        </FlyoutPage.Detail>
    
</FlyoutPage>